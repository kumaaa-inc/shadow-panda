{
  "name": "@shadow-panda/preset",
  "description": "The preset for PandaCSS that contains the theme for Shadow Panda",
  "version": "0.3.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "homepage": "https://shadow-panda.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kumaaa-inc/shadow-panda.git"
  },
  "bugs": {
    "url": "https://github.com/kumaaa-inc/shadow-panda/issues"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format=esm,cjs --dts",
    "build-fast": "tsup src/index.ts --format=esm,cjs --no-dts",
    "dev": "pnpm build-fast --watch"
  },
  "dependencies": {
    "@pandacss/preset-panda": "0.11.0"
  },
  "devDependencies": {
    "@pandacss/dev": "0.11.0",
    "@pandacss/types": "0.11.0"
  },
  "peerDependencies": {
    "@pandacss/dev": "0.11.0"
  }
}
